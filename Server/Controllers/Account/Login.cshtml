@model (string redirect, string errors)
<h1>Login</h1>

<form method="post" action=@($"/account/postlogin?uri={Model.redirect}")>
    <fieldset>
        <label for="username">Username</label>
        <input name="username" id="username">
    </fieldset>
    <fieldset>
        <label for="password">Password</label>
        <input name="password" id="password" type="password">
    </fieldset>
    <input type="submit">
</form>