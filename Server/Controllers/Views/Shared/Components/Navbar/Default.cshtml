@model List<AppTemplate.Controllers.NavItem>

<div class="flex flex-col border-r-2 border-black">
    @foreach(var item in Model)
    {
        <div class="m-4 p-2 border border-black shadow-md rounded-md grid place-items-center">
            <a class="link link-hover" hx-boost href="@item.Link">
                @item.Icon
            </a>
        </div>
    }
</div>